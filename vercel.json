{
  "version": 2,
  "builds": [
    {
      "src": "herens-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/((?!api|_next/static|_next/image|favicon.ico|assets|src).*)",
      "dest": "/herens-frontend/dist/index.html"
    }
  ]
}